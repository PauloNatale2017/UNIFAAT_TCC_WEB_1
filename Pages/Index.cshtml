@page
@model IndexModel.Salvar
@{
    ViewData["Title"] = "MAPA";
}


<script src="~/js/jquery-3.2.1.js"></script>
<script src="~/js/angular.js"></script>
<script src="~/js/crossfilter.js"></script>
<script src="~/js/fusioncharts.js"></script>
<script src="~/js/angular-fusioncharts.js"></script>
<script src="~/js/angular-fusioncharts.min.js"></script>
<script src="~/js/fusioncharts.theme.fusion.js"></script>
<script src="~/js/fusioncharts.maps.js"></script>
<script src="~/js/ng-map.min.js"></script>
<script src="~/js/angular-block-ui.js"></script>
<link href="~/css/angular-block-ui.css" rel="stylesheet" />

<script type="text/javascript"
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD21fXMDeTrDmxLC5yOPWBjM9fOOR20JBY&sensor=true">
</script>

<body ng-controller="CtrlMapsInfracoes">
    <div class="container" style="background-color:white;width:100%;height:800px">

        <div class="row">
            <div class="col-sx-12" style="width:100%;height:250px;background-color:white;display:flex">
                <div style="background-color:white;width: 40%;height: 194px;padding: 2px 2px 2px 2px;">
                     <div id="chartPie">FUSIONCHARTS XT WILL LOAD HERE!</div>
                </div>
                <div style="background-color:white;width: 60%;height: 194px;padding: 2px 2px 2px 2px;">
                    <div id="chartColumn">FUSIONCHARTS XT WILL LOAD HERE!</div>
                </div>
            </div>
        </div>
        <div class="row" style="margin:50px 1px 20px 1px;">
            <span style="float: left; text-align: center;width: 100%;font-style: oblique;font-family: serif;font-size: 120%;font-weight: bolder;">
                  VITIMAS CADASTRADAS COM CADASTROS COMPLETOS E INCOMPLETOS
            </span>
        </div>
        <div class="row" style=" overflow: scroll;">
            <table class="table">
                <thead class="thead-dark">
                    <tr>
                        <th>Nome Comppleto</th>
                        <th>Email</th>
                        <th>Contato</th>
                        <th>Endereco</th>
                    </tr>
                </thead>

                <tbody ng-repeat="itens in basic">
                    <tr>
                        <td>{{itens.NomeCompleto}}</td>
                        <td>{{itens.Email}}</td>
                        <td>{{itens.Contato}}</td>
                        <td>{{itens.Endereco}}</td>
                    </tr>
                </tbody>
            </table>
            @*<div id="map_canvas" class="col-xl-12" style="width:100%;height:350px;margin-top: 52px; float: left;">
        </div>*@
        </div>
        <div class="row">
            <div class="col-xl-12" style="width:100%;height:250px;background-color:white;">
            </div>
        </div>

    </div>
</body>


@section Scripts{
    <script src="~/js/IndexAngular.js"></script>
}
